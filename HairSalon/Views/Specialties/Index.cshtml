@{
  Layout="_Layout";
}
<br>
<div class="container-fluid">

<h3> Specialty List</h3>
<hr>

@if (Model.Count != 0)
{
  <ul class="list-group">
    @foreach (var specialty in Model)
    {
      <li class="list-group-item"><a href="/specialties/@specialty.GetSpecialtyId()">@specialty.GetSpecialty()</a> | <a href="/specialties/@specialty.GetSpecialtyId()/delete">Delete</a></li>
    }
  </ul>
}
<br>
<hr>
<br>
<h4><a href="/specialties/new">Add a new specialty</a></h4>
</div>
</div>
