@{
  Layout="_Layout";
}

    <ol>
      @foreach(var stylist in Model)
      {
        <li><h6><a href ="/stylists/details/@stylist.GetId()">@stylist.GetName()</a></h6></li>
      }
  </ol>
  <br>
    <a href="/stylists/new">Add a Stylist</a>
    <br>
    <br>
    <form action="/stylists/delete" method="post">
    <button class="btn btn-danger" type="submit" name="button">Clear All Stylists</button>
    </form>
