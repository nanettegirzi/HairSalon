@{
  Layout="_Layout";
}
<br>
<div class="container-fluid">

<h3> Stylist List</h3>
  <hr>
@if (Model.Count != 0)
{
  <ul class="list-group">
    @foreach (var stylist in Model)
    {
      <li class="list-group-item"><a href="/stylists/@stylist.GetStylistId()">@stylist.GetStylistName()</a>  -      <a href="/stylists/@stylist.GetStylistId()/update">Edit</a> | <a href="/stylists/@stylist.GetStylistId()/delete">Delete</a></li>
    }
  </ul>
}
<br>
<hr>
<br>
<h5><a href="/stylists/new">Add a new stylist</a></h5>

</div>
